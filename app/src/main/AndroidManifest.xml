<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xiaocydx.sample">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Recycler">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".itemclick.ItemClickActivity"
            android:exported="false" />
        <activity
            android:name=".itemselect.SingleSelectionActivity"
            android:exported="false" />
        <activity
            android:name=".itemselect.MultiSelectionActivity"
            android:exported="false" />
        <activity
            android:name=".multitype.MultiTypeActivity"
            android:exported="false" />
        <activity
            android:name=".paging.PagingActivity"
            android:exported="false" />
        <activity
            android:name=".viewpager.ViewPagerActivity"
            android:exported="false" />
    </application>
</manifest>